<idea-plugin>
  <id>github.cweijan.plugin.http.test</id>
  <name>Http Test Support</name>
  <version>1.0</version>
  <vendor email="cweijan@163.com" url="">cweijan</vendor>

  <description><![CDATA[
    This plugin create many context action to enhance development experience:
    1. Make all field to private.
    2. Bind all lombok annotation to class.
    3. Create http test to context action.
]]></description>

  <idea-version since-build="183"/>

  <!-- https://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/plugin_compatibility.html -->
  <depends>com.intellij.modules.platform</depends>
  <depends>com.intellij.modules.java</depends>

  <extensions defaultExtensionNs="com.intellij">
    <intentionAction>
      <className>github.cweijan.http.test.action.HttpTestForMethodAction</className>
    </intentionAction>
    <intentionAction>
      <className>github.cweijan.http.test.action.HttpParamCreateAction</className>
    </intentionAction>
    <intentionAction>
      <className>github.cweijan.http.test.action.HttpTestForClassAction</className>
    </intentionAction>
      <intentionAction>
          <className>github.cweijan.http.test.action.LombokCreateAction</className>
      </intentionAction>
    <intentionAction>
      <className>github.cweijan.http.test.action.FieldToPrivateAction</className>
    </intentionAction>
  </extensions>

  <actions>
    <!-- Add your actions here -->
  </actions>

</idea-plugin>